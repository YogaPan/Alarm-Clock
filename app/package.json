{
  "name": "alarm-clock-web",
  "version": "0.0.1",
  "description": "alarm clock web",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint ."
  },
  "keywords": [
    "alarm",
    "clock",
    "web"
  ],
  "author": "YogaPan",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "yoda": 0,
      "semi": [
        2,
        "always"
      ],
      "no-unused-vars": 2,
      "no-extra-semi": 2,
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1
    }
  }
}
