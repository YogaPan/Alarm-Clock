{
  "name": "alarm-clock",
  "productName": "Alarm Clock",
  "version": "0.0.1",
  "description": "alarm clock",
  "author": "YogaPan",
  "license": "MIT",
  "main": "app/index.js",
  "keywords": [
    "alarm",
    "clock"
  ],
  "scripts": {
    "start": "electron .",
    "test": "npm run lint",
    "dev": "webpack --watch",
    "lint": "eslint .",
    "build": "electron-packager . --all",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "dependencies": {
    "aphrodite": "^0.5.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "seamless-immutable": "^6.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "electron-packager": "^7.3.0",
    "electron-prebuilt": "^1.2.8",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "husky": "^0.11.5",
    "webpack": "^1.13.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-console": 0,
      "no-param-reassign": 0,
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "arrow-body-style": 0,
      "new-cap": 0,
      "consistent-return": 0
    },
    "settings": {
      "import/core-modules": [
        "electron"
      ]
    }
  }
}
